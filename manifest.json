{
    "manifest_version": 3,
    "name": "URL Groups",
    "description": "自动将具有相同一级URL前缀的标签页分组",
    "version": "1.1",
    "permissions": ["tabs", "tabGroups", "windows", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  
  